    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1",
    "groq-sdk": "^0.3.2",
    "resend": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
